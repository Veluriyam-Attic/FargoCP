	FORCE: {
		DisplayName: F.O.R.C.E.
		Tooltip:
			'''
			"The Fully Organizable Receptacle for Combining Enchantments.
			Its core of pure, crystallized Fargonium stabilizes the chaotic
			energies of opposing Enchantments, allowing them to coexist."
			'''
	}

	OmniBucket: {
		DisplayName: 全能桶 (水)
